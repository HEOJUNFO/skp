<!-- 첫 화면 (로딩 화면)은 실제 페이지 로딩 시간과는 무관하게 무조건 2초 후에 다음 페이지로 이동하도록 개발 반영 부탁 드립니다. (디자인 측 요청) -->
<div class="skp_loading">
    <img src="../../../resources/img/img_loading.gif" class="img_block" alt="로딩 이미지 영역">
    <h1 class="txt02">즐거운 서베이 시작해요</h1>
    <h2>survey GO</h2>
</div>

<div class="skp_wrap mainArea" style="display: none;">
    <!-- skip navigation -->
    <div class="skip_nav">
        <ul>
            <li><a href="#skp_container" id="scrollTop">본문 콘텐츠 영역으로 바로가기</a></li>
        </ul>
    </div>
    <!--// skip navigation -->
    <hr>

    <!-- head progress -->
    <header class="head_progress">
        <div class="prog_bar_box">
            <label for="ing">설문진행 상황</label>
            <progress id="ing" max="100" value="10">10%</progress>
            <img src="../../../resources/img/ico_3d_giftbox.gif" class="deco_ico nomalDecoIco" alt="장식 아이콘 이미지 영역">
            <img src="../../../resources/img/ico_3d_fin.png" class="deco_ico lastDecoIco" alt="장식 아이콘 이미지 영역" style="display: none;"> <!-- 완료 시 장식 아이콘 -->
        </div>
    </header>
    <!--// head progress -->
    <hr>

    <!-- container -->
    <div id="skp_container" class="skp_container subpage_type">

    </div>
    <!--// container -->
    <hr>

    <!-- foot -->
    <footer class="skp_foot" id="foot_area">

    </footer>
    <!--// foot -->

    <div class="toast_popup customNormalToast" style="display: none;">
        <p class="toast-text"></p>
    </div>

    <div class="toast_popup customAndAlertToast" style="display: none;">
        <p><img src="../../../resources/img/ico_3d_hand.png" class="deco_ico" alt="핸드 아이콘 이미지">3문항 남았어요!</p>
    </div>

    <!-- complex toast popup -->
    <div class="comp_toast_wrap" style="display:none;">
        <div class="comp_toast_popup">
            <div class="pop_result_box">
                <img src="../../../resources/img/img_3d_clap.gif" class="img_block answerResultTrue" alt="결과 이미지">
                <h1 class="answerResultTrue">정답입니다!</h1>

                <img src="../../../resources/img/img_3d_fail.gif" class="img_block answerResultFalse" alt="결과 이미지">
                <h1 class="answerResultFalse">틀렸습니다!</h1>
                <p class="g_txt answerResultFalse answerRetryTxt">다시 시도해보세요.</p>
            </div>
        </div>
    </div>
    <!--// complex toast popup -->

    <!-- modal popup -->
    <!-- 이미지 스와이프 리스트 모달 팝업 -->
    <div class="skp_modal_popup subjectPopupImgList-area" style="display: none;">
        <div class="skp_dimmed_layer"></div>
        <div class="skp_modal_inner">
            <button type="button" id="btn_pop_closed" class="pop_closed modalPopup-close popupImgClose">모달 팝업 닫기</button>
            <div class="sw_gal_wrap">
                <div class="swiper sw_img_gal">
                    <div class="swiper-wrapper subjectPopupImg-area">
                        <div class="swiper-slide"><img src="" class="img-block" alt="이미지 정보 영역"></div>
                        <div class="swiper-slide"><img src="" class="img-block" alt="이미지 정보 영역"></div>
                        <div class="swiper-slide"><img src="" class="img-block" alt="이미지 정보 영역"></div>
                        <div class="swiper-slide"><img src="" class="img-block" alt="이미지 정보 영역"></div>
                    </div>
                    <div class="swiper-pagination"></div>
                </div>
            </div>
        </div>
    </div>
    <!--// modal popup -->

    <!-- modal popup -->
    <!-- 힌트 / 정답설명 / 오답설명 모달팝업 -->
