                                <input type="radio" id="age02" class="frm_chk st07 attendAge" name="age" value="2">
                                <label for="age02">20대</label>
                            </div>
                            <div class="item">
                                <input type="radio" id="age03" class="frm_chk st07 attendAge" name="age" value="3">
                                <label for="age03">30대</label>
                            </div>
                            <div class="item">
                                <input type="radio" id="age04" class="frm_chk st07 attendAge" name="age" value="4">
                                <label for="age04">40대</label>
                            </div>
                            <div class="item">
                                <input type="radio" id="age05" class="frm_chk st07 attendAge" name="age" value="5">
                                <label for="age05">50대</label>
                            </div>
                            <div class="item">
                                <input type="radio" id="age06" class="frm_chk st07 attendAge" name="age" value="6">
                                <label for="age06">60대 이상</label>
                            </div>
                        </div>
                    </div>
                    <!--// radio &amp; checkbox box -->
                </dd>
            </dl>
        </div>
        <div class="inner_footer guide_effect">
            <div class="cmm_ok_module">
                <button type="button" class="attendTargetCheck">확인</button>
            </div>
        </div>
    </div>
    <!--// full overlay page -->

    <!-- full overlay page -->
    <!-- 경품추첨 요청 화면 -->
    <div class="full_overlay_page surveyPrize-area" style="display: none;">
        <div class="inner_content">
            <div class="msg_img_box">
                <span class="img_block -gift"><img src="../../../resources/img/img_3d_giftbox.gif" alt="일러스트 이미지 영역"></span>
                <h1>경품 추첨이 남았어요!</h1>
                <p class="g_txt">설문이 완료되었어요!</p>
