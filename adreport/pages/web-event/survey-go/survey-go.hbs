<script id="subjectTemplate" type="text/x-handlebars-template">
    <div class="content subject-area" data-survey-subject-id="\{{surveySubjectId}}" data-answer-result="N">
        \{{#ifvalue imgVideoYn value='Y'}}
        \{{#ifvalue imgVideoRegType value='IMG'}}
        <div class="skp_img_grp subjectPopupImgList-show">
            <span class="img_block">
                \{{#if popupImageInfo}}
                <button type="button" class="btn_more">이미지 더보기</button>
                \{{/if}}
                <img src="\{{contentUrl}}" alt="이미지 영역">
            </span>
        </div>
        \{{/ifvalue}}

        \{{#ifvalue imgVideoRegType value='VIDEO'}}
        <div class="video_player_box">
            <iframe id="youtube_sample_iframe"
              width="100%"
              src="\{{contentUrl}}"
              frameborder="0"
              allow="autoplay; fullscreen"
              allowfullscreen>
            </iframe>
        </div>
        \{{/ifvalue}}
        \{{/ifvalue}}
        <div class="skp_tit_grp">
            <h1>\{{subjectTitle}}</h1>
            \{{#if subjectSubText}}
            <p class="g_txt">\{{subjectSubText}}</p>
            \{{/if}}
        </div>

        \{{#ifvalue subjectExampleType value='CHOICE'}}
        <!-- 객관식 -->
            \{{#ifvalue exampleChoiceType value='WIDTH'}}
            <!-- 가로형 -->
            <ul class="g_list grd_col_type">
                \{{#each exampleInfo}}
                <li>
                    <input type="\{{#ifvalue ../multipleAnswerYn value='Y'}}checkbox\{{else}}radio\{{/ifvalue}}"
                            id="choice\{{indexToOrderNoId @index}}" name="mc_group"
                            class="frm_chk \{{#ifvalue ../multipleAnswerYn value='Y'}}st01\{{/ifvalue}} exampleInput"
                            value="\{{sort}}">
                    <label for="choice\{{indexToOrderNoId @index}}"><span>\{{exampleTitle}}</span></label>
                </li>
                \{{/each}}

                \{{#ifvalue etcOpinionReceiveYn value='Y'}}
                <li class="one_col_item">
                    <input type="checkbox" id="frm_chk_etc" name="\{{surveySubjectId}}-etc" class="frm_chk st02">
                    <label for="frm_chk_etc"><span>기타</span></label>
                    <div class="expanded_edit_box customTextareaDiv">
                        <!-- TODO : textarea에서 입력 완료 후에는 done 클래스 추가 (다시 해당 영역에 터치하여 native keyboard layout 노출 시 반대로 클래스 제거) -->
                        <textarea cols="90" rows="1" id="textfield" placeholder="내용을 입력해 주세요." class="done etcOpinionReceive customTextareaInput" maxlength="5000"></textarea>
                        <div class="count"><em>0</em> / 5,000</div>
                    </div>
                </li>
                \{{/ifvalue}}
            </ul>
            \{{/ifvalue}}


            \{{#ifvalue exampleChoiceType value='HEIGHT'}}
            <!-- 세로형 -->
            <ul class="g_list">
                \{{#each exampleInfo}}
                <li>
                    <input type="\{{#ifvalue ../multipleAnswerYn value='Y'}}checkbox\{{else}}radio\{{/ifvalue}}"
                        id="choice\{{indexToOrderNoId @index}}" name="mc_group"
                        class="frm_chk \{{#ifvalue ../multipleAnswerYn value='Y'}}st01\{{/ifvalue}} exampleInput"
                        value="\{{sort}}">
                    <label for="choice\{{indexToOrderNoId @index}}"><span>\{{exampleTitle}}</span></label>
                </li>
                \{{/each}}

                \{{#ifvalue etcOpinionReceiveYn value='Y'}}
                <li>
                    <input type="radio" id="frm_chk_etc" name="\{{surveySubjectId}}-etc" class="frm_chk st02">
                    <label for="frm_chk_etc"><span>기타</span></label>
                    <div class="expanded_edit_box customTextareaDiv">
                        <!-- TODO : textarea에서 입력 완료 후에는 done 클래스 추가 (다시 해당 영역에 터치하여 native keyboard layout 노출 시 반대로 클래스 제거) -->
                        <textarea cols="90" rows="1" id="textfield" placeholder="내용을 입력해 주세요." class="done etcOpinionReceive customTextareaInput" maxlength="5000"></textarea>
                        <div class="count"><em>0</em> / 5,000</div>
                    </div>
                </li>
                \{{/ifvalue}}
            </ul>
            \{{/ifvalue}}

            \{{#ifvalue exampleChoiceType value='IMG'}}
            <!-- 이미지형 -->

            <ul class="g_list grd_photo_type">
                \{{#each exampleInfo}}
                <li>
                    <input type="\{{#ifvalue ../multipleAnswerYn value='Y'}}checkbox\{{else}}radio\{{/ifvalue}}"
                        id="choice\{{indexToOrderNoId @index}}" name="mc_group"
                        class="frm_chk st03 exampleInput"
                        value="\{{sort}}">
                    <label for="choice\{{indexToOrderNoId @index}}">
                    <span class="img_block"><img src="\{{exampleImgUrl}}" alt="썸네일 이미지 영역"></span>
                    <span>\{{exampleTitle}}</span>
                    </label>
                </li>
                \{{/each}}
            </ul>
            \{{/ifvalue}}

            \{{#ifvalue exampleChoiceType value='SCALE'}}
            <!-- 척도형 -->
            <div class="point_box_grp">
                <!-- box port -->
                <div class="point_box_port">
                    <h2>\{{scaleText}}</h2>

                    \{{#ifvalue scaleViewType value='STAR'}}
                    <!-- 별점 -->
                    <div class="rating_box">
                        <form name="rating" method="post" action="">
                            <fieldset>
                                <legend>별점 평가</legend>
                                <input type="radio" name="rating" value="5" id="rate01" class="scaleRadio"><label for="rate01">⭐</label>
                                <input type="radio" name="rating" value="4" id="rate02" class="scaleRadio"><label for="rate02">⭐</label>
                                <input type="radio" name="rating" value="3" id="rate03" class="scaleRadio"><label for="rate03">⭐</label>
                                <input type="radio" name="rating" value="2" id="rate04" class="scaleRadio"><label for="rate04">⭐</label>
                                <input type="radio" name="rating" value="1" id="rate05" class="scaleRadio"><label for="rate05">⭐</label>
                            </fieldset>
                        </form>
                    </div>

                    \{{else}}
                    <div class="box-inner">
                        \{{#each exampleInfo}}
                        \{{#ifvalue ../scaleViewType value='EXPRESSION'}}
                        <!-- 표정 -->
                        \{{#if @first}}
                        <div class="item">
                            <input type="radio" id="worst01" name="points01_group" class="frm_chk st04 scaleRadio" value="\{{sort}}">
                            <label for="worst01">
                            <span class="img_block"><img src="../../../resources/img/ico_standard01.png" alt="매우 별로를 표현하는 아이콘 이미지"></span>
                            <span>\{{exampleTitle}}</span>
                            </label>
                        </div>
                        \{{/if}}

                        \{{#ifvalue ../exampleCount value=3}}
                            \{{#ifvalue @index value=1}}
                            <div class="item">
                                <input type="radio" id="not_bad01" name="points01_group" class="frm_chk st04 scaleRadio" value="\{{sort}}">
                                <label for="not_bad01">
                                <span class="img_block"><img src="../../../resources/img/ico_standard03.png" alt="보통을 표현하는 아이콘 이미지"></span>
