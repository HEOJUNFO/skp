<!-- 참여번호 팝업 -->
<section id="attendCodePopupSection" style="display: none;">
    <!-- 팝업  -->
    <div class="layer_popup">
        <div class="popup_contents">
            <div class="pfl_pop">
                <span class="pop_title">참여 번호 입력</span>

                <p>
                    이벤트 지역에서 발급받은 참여 번호를<br>입력해 주세요.
                </p>

                <div class="info_form">
                    <ul>
                        <!-- 참여 번호 활성화 -->
                        <li class="on">
                            <span class="title moved_fix"><a href="#">참여번호</a></span>
                            <div class="auth_number">			
                                <input type="password" class="contents_input" id="attendCode">
                            </div>
                            <span class="error_msg" id="errorMsg"></span><!-- 참여 번호가 정확하지 않습니다. 다시 입력해 주세요. -->
                        </li>
                        <!-- // 참여 번호 활성화 -->
                    </ul>
                </div>
            </div>
        </div>

        <div class="btn two">
            <button class="grey" id="attendCodeCancelBtn">취소</button>
            <button class="confirm" id="attendCodeValidateionBtn">확인</button>
        </div>
    </div>
    <!-- // 팝업 -->
    <!-- 딤드 배경 -->
    <div class="dimmed_bg_lightdark"></div>
    <!-- // 딤드 배경 -->
</section>
